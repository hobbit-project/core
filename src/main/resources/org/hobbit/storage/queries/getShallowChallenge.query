    %CHALLENGE_URI% a hobbit:Challenge .
	%CHALLENGE_URI% ?challengeProp ?challengeObj .
}
WHERE {
  GRAPH %GRAPH_URI% {
    %CHALLENGE_URI% a hobbit:Challenge .
	%CHALLENGE_URI% ?challengeProp ?challengeObj .
	}
}